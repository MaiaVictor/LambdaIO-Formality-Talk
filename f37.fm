// O critério da análise de casos
// ==============================

bool_to_string(bool: Bool): String
  case bool:
  | true  => "true";
  | false => "false";
  : String;

unit_to_string(unit: Unit): String
  case unit:
  | unit => "unit";
  : String;

empty_to_string(empty: Empty): String
  case empty:
  : String;
